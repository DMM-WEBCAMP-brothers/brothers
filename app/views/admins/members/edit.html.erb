登録情報編集
<form>
    <div class="row">
      <div class="col-xs-2">
        <%= f.label :"名前" %>
      </div>
      <div class="col-xs-3 field">
        <%= f.label :"(姓)" %>
        <%= f.text_field :last_name, autofocus: true, class: "last_name" %>
      </div>
      <div class="col-xs-3 field">
        <%= f.label :"(名)" %>
        <%= f.text_field :first_name, autofocus: true, class: "first_name" %>
      </div>
    </div>

    <div class="row">
      <div class="col-xs-2">
        <%= f.label :"フリガナ" %>
      </div>
      <div class="col-xs-3 field">
        <%= f.label :"(セイ)" %>
        <%= f.text_field :last_name_kana, autofocus: true, class: "last_name_kana" %>
      </div>
      <div class="col-xs-3 field">
        <%= f.label :"(メイ)" %>
        <%= f.text_field :first_name_kana, autofocus: true, class: "first_name_kana" %>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-2">
        <%= f.label :"郵便番号(ハイフンなし)" %>
      </div>
      <div class="col-xs-3 field">
        <%= f.text_field :postcode %>
      </div>
    </div>

    <div class="row">
      <div class="col-xs-2">
        <%= f.label :"住所" %>
      </div>
      <div class="col-xs-6 field">
        <%= f.text_field :address %>
      </div>
    </div>

    <div class="row">
      <div class="col-xs-2">
        <%= f.label :"電話番号(ハイフンなし)" %>
      </div>
      <div class="col-xs-3 field">
        <%= f.text_field :phone_number %>
      </div>
    </div>

    <div class="row">
      <div class="col-xs-2">
        <%= f.label :"メールアドレス" %>
      </div>
      <div class="col-xs-3 field">
        <%= f.email_field :email, class: "email" %>
      </div>
    div class="row">

      <div class="col-xs-2">
        <%= f.label :"会員ステータス" %>
      </div>
      <div class="col-xs-3 field">
        div class="row">
      <div class="col-xs-2">
      <label><input type="radio" name="radio">有効</label>
      <label><input type="radio" name="radio">退会済み</label>
      </div>
      
    <div class="row">
      <div class="actions">
        <%= f.submit "Sign up" ,class: 'btn btn-success' %>
      </div>
    </div>
  </form>
<% end %>
